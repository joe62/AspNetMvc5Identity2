
@{
    ViewBag.Title = "主页";
}
<div class="panel panel-primary">
    <div class="panel-heading">用户信息</div>
    <table class="table table-striped">
        @foreach (string key in Model.Keys) { 
            <tr>
                <th>@key</th>
                <td>@Model[key]</td>
            </tr>
        }
    </table>
</div>
@Html.ActionLink("登出","Logout","Account",null,new{@class="btn btn-primary"})
